@prefix : <http://www.knora.org/ontology/knora-base#> .
@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xml: <http://www.w3.org/XML/1998/namespace> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@base <http://www.knora.org/ontology/knora-base> .

<http://www.knora.org/ontology/knora-base> rdf:type owl:Ontology .

#
## "Forward imports as knora-base.ttl is loaded after this file"
#

:systemProperty rdf:type rdf:Property .
:knoraProject rdf:type owl:Class .
:UserGroup rdf:type owl:Class .




#
##
### Base Permission Property
##
#
###  http://www.knora.org/ontology/knora-base#hasPermission

:hasPermission rdf:type rdf:Property ;

               rdfs:comment "Base permission property."@en ;
               
               rdfs:subPropertyOf :systemProperty .



#
##
### Permission Class Properties
##
#
### http://www.knora.org/ontology/knora-base#forProject

:forProject rdf:type rdf:Property ;

            rdfs:comment "Permission property pointing to a knoraProject."@en ;

            rdfs:subPropertyOf :systemProperty .



### http://www.knora.org/ontology/knora-base#forGroup

:forGroup rdf:type rdf:Property ;

          rdfs:comment "Permission property pointing to a UserGroup."@en ;

          rdfs:subPropertyOf :systemProperty .



### http://www.knora.org/ontology/knora-base#forResourceClass

:forResourceClass rdf:type rdf:Property ;

                  rdfs:comment "Permission property pointing to a resource class."@en ;

                  rdfs:subPropertyOf :systemProperty .



### http://www.knora.org/ontology/knora-base#forProperty

:forProperty rdf:type rdf:property ;

             rdfs:comment "Permission property pointing to a resource property."@en ;

             rdfs:subPropertyOf :systemProperty .



#
##
### Permission Class
##
#
### http://www.knora.org/ontology/knora-base#Permission

:Permission rdf:type owl:Class ;

            rdfs:comment "Permission class."@en.



### http://www.knora.org/ontology/knora-base#ProjectNotApplicable

:ProjectNotApplicable rdf:type :knoraProject ;

                      rdfs:comment "A built-in project instance used in permission instances."@en



### http://www.knora.org/ontology/knora-base#GroupNotApplicable

:GroupNotApplicable rdf:type :UserGroup ;

                    rdfs:comment "A built-in group instance used in permission instances."@en



### http://www.knora.org/ontology/knora-base#ResourceClassNotApplicable

:ResourceClassNotApplicable rdf:type owl:Class ;

                            rdfs:comment "A built-in resource class used in permission instances."@en



### http://www.knora.org/ontology/knora-base#PropertyNotApplicable

:PropertyNotApplicable rdf:type owl:ObjectProperty ;

                       rdfs:comment "A built-in resource property used in permission instances."@en



#
##
### Object Access Permission Properties
##
#
###  http://www.knora.org/ontology/knora-base#hasObjectAccessPermission

:hasObjectAccessPermission rdf:type owl:ObjectProperty ;

                           rdfs:comment "Base Object Access Permission property."@en ;

                           rdfs:subPropertyOf :hasPermission .



###  http://www.knora.org/ontology/knora-base#hasChangeRightsPermission

:hasChangeRightsPermission rdf:type owl:ObjectProperty ;

                           :objectClassConstraint :UserGroup ;

                           rdfs:subPropertyOf :hasDeletePermission .



###  http://www.knora.org/ontology/knora-base#hasDeletePermission

:hasDeletePermission rdf:type owl:ObjectProperty ;

                     :objectClassConstraint :UserGroup ;

                     rdfs:subPropertyOf :hasModifyPermission .



###  http://www.knora.org/ontology/knora-base#hasModifyPermission

:hasModifyPermission rdf:type owl:ObjectProperty ;

                     :objectClassConstraint :UserGroup ;

                     rdfs:subPropertyOf :hasViewPermission .



###  http://www.knora.org/ontology/knora-base#hasViewPermission

:hasViewPermission rdf:type owl:ObjectProperty ;

                   :objectClassConstraint :UserGroup ;

                   rdfs:subPropertyOf :hasRestrictedViewPermission .



###  http://www.knora.org/ontology/knora-base#hasRestrictedViewPermission

:hasRestrictedViewPermission rdf:type owl:ObjectProperty ;

                             :objectClassConstraint :UserGroup ;

                             rdfs:subPropertyOf :hasObjectAccessPermission .



#
##
### Administrative Permission Value Class
##
#
### http://www.knora.org/ontology/knora-base#PermissionValue

:AdministrativePermissionValue rdf:type owl:Class ;

                               rdfs:comment "Administrative permission values class."@en.



#
##
### Administrative Permissions
##
#

###  http://www.knora.org/ontology/knora-base#hasDefaultObjectAccessPermission

:hasAdministrativePermission rdf:type owl:ObjectProperty ;

                             rdfs:comment "Base Administrative Permission property."@en ;

                             rdfs:subPropertyOf :hasPermission .



#
## Resource Creation Permission Value Class and Instance
#

###  Class
###  http://www.knora.org/ontology/knora-base#ResourceCreationPermissionValue

:ResourceCreationPermissionValue rdf:type owl:Class ;

                                 rdf:subClassOf :AdministrativePermissionValue ;
                                 
                                 rdfs:comment "Base Resource Creation Permission Value class."@en .


###  Instance
<http://www.knora.org/ontology/knora-base#ProjectResourceCreateAllPermission>

                        rdf:type :ResourceCreationPermissionValue ;

                        rdfs:comment "Gives the permission to create resources inside the project."@en .


#
## Resource Creation Permission Properties
#

###  http://www.knora.org/ontology/knora-base#hasResourceCreationPermission

:hasResourceCreationPermission rdf:type owl:ObjectProperty ;

                               rdfs:comment "Base Object Access Permission property."@en ;

                               rdfs:subPropertyOf :hasAdministrativePermission ;

                               :subjectClassConstraint :Permission ;

                               :objectClassConstraint :ResourceCreationPermissionValue .



### http://www.knora.org/ontology/knora-base#hasRestrictedProjectResourceCreatePermission

:hasRestrictedProjectResourceCreatePermission rdf:type owl:ObjectProperty ;

                                              rdfs:comment "Gives the permission to create resources of the specified class inside the project."@en ;

                                              rdfs:subPropertyOf :hasResourceCreationPermission ;

                                              :subjectClassConstraint :Permission ;

                                              :objectClassConstraint owl:Class .



#
## Project Administration Permission Value Class and Instances
#

###  Class
###  http://www.knora.org/ontology/knora-base#ProjectAdministrationPermissionValue

:ProjectAdministrationPermissionValue rdf:type owl:Class ;

                                      rdf:subClassOf :AdministrativePermissionValue ;
                                      
                                      rdfs:comment "Base Project Administration Permission Value class."@en .



###  Instances
<http://www.knora.org/ontology/knora-base#ProjectAllAdminPermission>

                      rdf:type :ProjectAdministrationPermissionValue ;

                      rdfs:comment ""@en .



<http://www.knora.org/ontology/knora-base#ProjectAllGroupAdminPermission>

                      rdf:type :ProjectAdministrationPermissionValue ;

                      rdfs:comment ""@en .



<http://www.knora.org/ontology/knora-base#ProjectRightsAdminPermission>

                      rdf:type :ProjectAdministrationPermissionValue ;

                      rdfs:comment ""@en .



#
## Project Administration Permission Properties
#

### http://www.knora.org/ontology/knora-base#hasProjectAdministrationPermission

:hasProjectAdministrationPermission rdf:type owl:ObjectProperty ;

                                    rdfs:comment "Base Project Administration Permission property."@en ;

                                    rdfs:subPropertyOf :hasAdministrativePermission ;

                                    :subjectClassConstraint :Permission ;

                                    :objectClassConstraint :ProjectAdministrationPermissionValue .



### http://www.knora.org/ontology/knora-base#hasProjectRestrictedGroupAdminPermission

:hasProjectRestrictedGroupAdminPermission rdf:type owl:ObjectProperty ;

                                          rdfs:comment ""@en ;

                                          rdfs:subPropertyOf :hasPermission ;

                                          :objectClassConstraint :UserGroup .



#
## Ontology Administration Permissions Value Class and Instance
#

###  Class
###  http://www.knora.org/ontology/knora-base#OntologyAdministrationPermissionValue

:OntologyAdministrationPermissionValue rdf:type owl:Class ;

                                       rdf:subClassOf :AdministrativePermissionValue ;

                                       rdfs:comment "Base Project Administration Permission Value class."@en .



###  Instance
<http://www.knora.org/ontology/knora-base#ProjectOntologyAdminPermission

                      rdf:type OntologyAdministrationPermissionValue ;

                      rdfs:comment ""@en .



#
## Ontology Administration Permission Properties
#

### http://www.knora.org/ontology/knora-base#hasOntologyAdministrationPermission

:hasOntologyAdministrationPermission rdf:type owl:ObjectProperty ;

                                     rdfs:comment "Base Ontology Administration Permission property."@en ;

                                     rdfs:subPropertyOf :hasAdministrativePermission ;

                                     :subjectClassConstraint :Permission ;

                                     :objectClassConstraint :OntologyAdministrationPermissionValue .



#
##
### Default Object Access Permissions
##
#

###  http://www.knora.org/ontology/knora-base#hasDefaultObjectAccessPermission

:hasDefaultObjectAccessPermission rdf:type owl:ObjectProperty ;

                                  rdfs:comment "Base Default Object Access Permission property."@en ;

                                  rdfs:subPropertyOf :hasPermission .



###  http://www.knora.org/ontology/knora-base#hasDefaultChangeRightsPermission

:hasDefaultChangeRightsPermission rdf:type owl:ObjectProperty ;

                                  :objectClassConstraint :UserGroup ;

                                  rdfs:subPropertyOf :hasDefaultDeletePermission .



###  http://www.knora.org/ontology/knora-base#hasDefaultDeletePermission

:hasDefaultDeletePermission rdf:type owl:ObjectProperty ;

                            :objectClassConstraint :UserGroup ;

                            rdfs:subPropertyOf :hasDefaultModifyPermission .



###  http://www.knora.org/ontology/knora-base#hasDefaultModifyPermission

:hasDefaultModifyPermission rdf:type owl:ObjectProperty ;

                            :objectClassConstraint :UserGroup ;

                            rdfs:subPropertyOf :hasDefaultViewPermission .



###  http://www.knora.org/ontology/knora-base#hasDefaultViewPermission

:hasDefaultViewPermission rdf:type owl:ObjectProperty ;

                          rdfs:subPropertyOf :hasDefaultRestrictedViewPermission .



###  http://www.knora.org/ontology/knora-base#hasDefaultRestrictedViewPermission

:hasDefaultRestrictedViewPermission rdf:type owl:ObjectProperty ;

                                    :objectClassConstraint :UserGroup ;

                                    rdfs:subPropertyOf :hasDefaultObjectAccessPermission .
